# CloudFlow SaaS Template - Environment Variables

# Application
NODE_ENV=development
PORT=3000

# Analytics
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
GOOGLE_TAG_MANAGER_ID=GTM_XXXXXXX
FACEBOOK_PIXEL_ID=XXXXXXXXXX

# Form Submissions
FORMS_ID=your-forms-id
EMAILJS_PUBLIC_KEY=your-emailjs-public-key
EMAILJS_SERVICE_ID=your-emailjs-service-id
EMAILJS_TEMPLATE_ID=your-emailjs-template-id

# Newsletter
MAILCHIMP_API_KEY=your-mailchimp-api-key
MAILCHIMP_LIST_ID=your-mailchimp-list-id
CONVERTKIT_API_KEY=your-convertkit-api-key
CONVERTKIT_FORM_ID=your-convertkit-form-id

# Social Media
TWITTER_HANDLE=@yourcompany
FACEBOOK_PAGE=yourcompany
LINKEDIN_COMPANY=yourcompany
INSTAGRAM_HANDLE=@yourcompany

# Contact Information
CONTACT_EMAIL=hello@yourcompany.com
CONTACT_PHONE=+1-555-123-4567
COMPANY_ADDRESS=123 Business St, Suite 100, San Francisco, CA 94105

# SEO
SITE_URL=https://yourwebsite.com
SITE_NAME=Your Company Name
SITE_DESCRIPTION=Your company description
SITE_KEYWORDS=saas,software,business,productivity

# Security
CSP_NONCE=your-csp-nonce
HSTS_MAX_AGE=31536000

# Performance
CACHE_MAX_AGE=31536000
COMPRESSION_LEVEL=6

# Development
DEBUG=false
LOG_LEVEL=info
ENABLE_SOURCE_MAPS=true

# Build
BUILD_VERSION=1.2.0
BUILD_DATE=2024-01-15
BUILD_COMMIT=git-commit-hash 